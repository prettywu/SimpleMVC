<!DOCTYPE html>
<html lang="en">

<head>
    <title>Dashboard | Klorofil - Free Bootstrap Dashboard Template</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <!-- VENDOR CSS -->
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/font-lnr.css" rel="stylesheet" />
    <!-- ICONS -->
    <link rel="apple-touch-icon" sizes="76x76" href="/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" sizes="96x96" href="/assets/img/favicon.png">
    <style>
        body {
            padding-top: 50px;
            background-color:#f5f5fa;
        }

        #layout-navbar {
            background-color: white;
        }

        #sidebar-nav {
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
            width: 260px;
            height: calc(100% - 50px);
            float: left;
            background-color: #2B333E;
            position: fixed;
            left: 0;
            padding-bottom: 20px;
        }

        .nav i {
            margin-right: 10px;
            font-size: 16px;
        }

        .nav > li > a {
            padding: 15px 30px;
            color: #AEB7C2;
            border-left: 5px solid transparent;
        }

            .nav > li > a.active {
                background-color: #252c35;
                color: #fff;
            }

            .nav > li > a:hover, .nav > li > a:focus {
                background-color: #252c35;
                color: #fff;
            }

                .nav > li > a:hover i, .nav > li > a:focus i, .nav > li > a.active i {
                    color: #00AAFF;
                }

        nav > .nav > li > a.active {
            border-left-color: #00AAFF;
        }

        .nav > li > a[data-toggle="collapse"]:not(.collapsed) {
            background-color: #252c35;
            border-left-color: #00AAFF;
            color: #fff;
        }

            .nav > li > a[data-toggle="collapse"]:not(.collapsed) i {
                color: #00AAFF;
            }

        .nav > li > a[data-toggle="collapse"].collapsed .icon-submenu {
            -moz-transform: rotate(0deg);
            -ms-transform: rotate(0deg);
            -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
        }

        .nav > li > a[data-toggle="collapse"] .icon-submenu {
            /* display: inline-block; */
            /* vertical-align: middle; */
            -webkit-transition: all 0.2s ease-out;
            -moz-transition: all 0.2s ease-out;
            -ms-transition: all 0.2s ease-out;
            -o-transition: all 0.2s ease-out;
            transition: all 0.2s ease-out;
            float: right;
            position: relative;
            top: 5px;
            font-size: 12px;
            /* line-height: 1.1; */
            -moz-transform: rotate(-90deg);
            -ms-transform: rotate(-90deg);
            -webkit-transform: rotate(-90deg);
            transform: rotate(-90deg);
        }

        .nav .nav > li > a {
            padding-left: 60px;
        }

        .main {
            -webkit-transition: all 0.3s ease-in-out;
            -moz-transition: all 0.3s ease-in-out;
            -ms-transition: all 0.3s ease-in-out;
            -o-transition: all 0.3s ease-in-out;
            transition: all 0.3s ease-in-out;
            width: calc(100% - 260px);
            left: 0;
            position: relative;
            float: right;
                padding: 15px;
        }

        footer {
            position: absolute;
            bottom: 0;
            right: 0;
            padding-top: 15px;
            padding-bottom: 10px;
        }
    </style>
    @RenderSection("styles", required: false)
</head>

<body>
    <!-- WRAPPER -->
    <div id="layout">
        <!-- NAVBAR -->
        <nav class="navbar navbar-default navbar-fixed-top" id="layout-navbar">

        </nav>
        <!-- END NAVBAR -->
        <!-- LEFT SIDEBAR -->
        <div id="sidebar-nav" class="sidebar">
            <div class="sidebar-scroll">
                <nav>
                    <ul class="nav">
                        <li><a href="~/Admin/index" class="active"><i class="lnr lnr-home"></i> <span>Dashboard</span></a></li>
                        <li><a href="~/Admin/elements" class=""><i class="lnr lnr-code"></i> <span>Elements</span></a></li>
                        <li><a href="~/Admin/charts" class=""><i class="lnr lnr-chart-bars"></i> <span>Charts</span></a></li>
                        <li><a href="~/Admin/panels" class=""><i class="lnr lnr-cog"></i> <span>Panels</span></a></li>
                        <li><a href="~/Admin/notifications" class=""><i class="lnr lnr-alarm"></i> <span>Notifications</span></a></li>
                        <li>
                            <a href="#subPages1" data-toggle="collapse" class="collapsed">
                                <i class="lnr lnr-file-empty"></i>
                                <span>Pages</span>
                                <i class="icon-submenu lnr lnr-chevron-left"></i>
                            </a>
                            <div id="subPages1" class="collapse ">
                                <ul class="nav">
                                    <li><a href="~/Admin/page-profile" class="">Profile</a></li>
                                    <li><a href="~/Admin/login" class="">Login</a></li>
                                    <li><a href="~/Admin/page-lockscreen" class="">Lockscreen</a></li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <a href="#subPages2" data-toggle="collapse" class="collapsed">
                                <i class="lnr lnr-file-empty"></i>
                                <span>Pages</span>
                                <i class="icon-submenu lnr lnr-chevron-left"></i>
                            </a>
                            <div id="subPages2" class="collapse ">
                                <ul class="nav">
                                    <li><a href="~/Admin/page-profile" class="">Profile</a></li>
                                    <li><a href="~/Admin/login" class="">Login</a></li>
                                    <li><a href="~/Admin/page-lockscreen" class="">Lockscreen</a></li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <a href="#subPages3" data-toggle="collapse" class="collapsed">
                                <i class="lnr lnr-file-empty"></i>
                                <span>Pages</span>
                                <i class="icon-submenu lnr lnr-chevron-left"></i>
                            </a>
                            <div id="subPages3" class="collapse ">
                                <ul class="nav">
                                    <li><a href="~/Admin/page-profile" class="">Profile</a></li>
                                    <li><a href="~/Admin/login" class="">Login</a></li>
                                    <li><a href="~/Admin/page-lockscreen" class="">Lockscreen</a></li>
                                </ul>
                            </div>
                        </li>
                        <li><a href="~/Admin/tables" class=""><i class="lnr lnr-dice"></i> <span>Tables</span></a></li>
                        <li><a href="~/Admin/typography" class=""><i class="lnr lnr-text-format"></i> <span>Typography</span></a></li>
                        <li><a href="~/Admin/icons" class=""><i class="lnr lnr-linearicons"></i> <span>Icons</span></a></li>
                    </ul>
                </nav>

            </div>
        </div>
        <!-- END LEFT SIDEBAR -->
        <!-- MAIN -->
        <div class="main">
            <!-- MAIN CONTENT -->
            <div class="main-content">
                @RenderBody()
            </div>
            <!-- END MAIN CONTENT -->
            <footer>
                <div class="container-fluid">
                    <p class="copyright">&copy; 2017 <a href="#" target="_blank">Theme I Need</a>.
                </div>
            </footer>
        </div>
        <!-- END MAIN -->
        
        <div class="clearfix"></div>
        
    </div>
    <!-- END WRAPPER -->
    <!-- Javascript -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/jquery.slimscroll.js"></script>
    <script>
        $(function () {
            $('.sidebar-scroll').slimScroll({
                height: 'auto',
                color: '#00AAFF'
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</body>

</html>
